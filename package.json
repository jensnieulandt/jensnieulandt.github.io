{
  "name": "jensnieulandt.github.io",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "commit": "cz",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:style && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix && pnpm lint:style --fix",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "is-ci": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@nuxt-themes/elements": "^0.4.4",
    "@nuxt-themes/typography": "^0.4.3",
    "@nuxt/content": "^2.7.0",
    "@nuxthq/studio": "^0.6.6",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "lint-staged": "^13.2.2",
    "nuxt": "3.4.3",
    "nuxt-config-schema": "^0.4.6",
    "nuxt-icon": "^0.2.11",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sitemap": "^7.1.1",
    "stylelint": "15.8.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^4.9.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.1",
    "webpack": "^5.88.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  }
}
